<!DOCTYPE html>
<html>
  <head>
    <title> <%= yield(:title)%></title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="container">
      <div id="header">
        <%= link_to "Lets Ask", root_path %>
      </div><!-- end header-->
      <div id="nav">
        <ul>
          <li> <%= link_to "Home", root_path %></li>
        </ul>
      </div><!-- end of nav-->
      <div id="content">
        <% flash.each do |key, value| %>
        <p id = "message"> <%= value %></p>
        <% end %>
        <%= yield %>
      </div>
      <div id="footer">
        &copy; <%= link_to "Raj Anand", "http://github.com/rajanand02" %> <%= Date.today %>
      </div><!-- end footer-->
    </div><!-- end container-->
  </body>
</html>
